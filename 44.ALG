algoritmo "semnome"
var
   valorInicial, valorFinal, incremento: inteiro
inicio
   escreval("Primeiro valor: ")
   leia(valorInicial)
   escreval("Valor final ")
   leia(valorFinal)
   escreval("Incremento ")
   leia(incremento)


   enquanto valorInicial <= valorFinal faca
      escreva(valorInicial)
      valorInicial <- valorInicial + incremento
   fimenquanto




fimalgoritmo