algoritmo "semnome"
var
   a, b, soma: inteiro
funcao somador(x,y: inteiro) : inteiro
inicio
   soma <- a + b
   retorne soma
fimfuncao
inicio
   escreval("Digite um numero: ")
   leia(a)
   escreval("Digite outro numero: ")
   leia(b)
   soma <- somador(a,b)
   escreval("A soma entre ", a , " e ", b, " e igual a: ", soma)

fimalgoritmo